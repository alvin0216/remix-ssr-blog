{
  "private": true,
  "name": "remix-ssr-blog",
  "description": "Personal blog developed based on remix",
  "license": "MIT",
  "scripts": {
    "postinstall": "remix setup node",
    "start": "cross-env PORT=3333 remix-serve build",
    "build": "concurrently \"npm run seed:config\" \"remix build\"",
    "build:css": "unocss \"app/routes/**/*.tsx\"",
    "dev": "concurrently \"npm run seed:config\" \"npm run dev:css\" \"cross-env PORT=3333 remix dev\"",
    "dev:css": "unocss -o ./app/styles/uno.css \"app/**/*.tsx\" --watch",
    "database": "npx prisma db push",
    "seed:config": "ts-node app/utils/parseYml.server.ts",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.2.1",
    "@remix-run/serve": "^1.2.1",
    "@types/fs-extra": "^9.0.13",
    "antd": "^4.18.5",
    "axios": "^0.26.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.0",
    "highlight.js": "^11.4.0",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remark-mdx-frontmatter": "^1.1.1",
    "remix": "^1.2.1",
    "remix-auth": "^3.2.1",
    "remix-auth-github": "^1.0.0",
    "xss": "^1.0.10",
    "yaml": "^1.10.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.1",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/yamljs": "^0.2.31",
    "@unocss/cli": "^0.22.6",
    "concurrently": "^7.0.0",
    "esbuild-dynamic-import-plugin": "^0.0.12",
    "prisma": "^3.8.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2",
    "unocss": "^0.22.6"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
